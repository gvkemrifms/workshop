@model IEnumerable<Fleet_WorkShop.Models.VehicleReport>

<link href="~/ScriptsCss/Bindings.css" rel="stylesheet"/>
<h1 style="color: brown;" align="center"><u>Vehicle Wise Stock Report</u></h1>

<table class="table" align="center">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Workshop)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Vehicle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Sparepart)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Quantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Amount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.JobcardId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HandOverTo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IssuedDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Workshop)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Vehicle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sparepart)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Quantity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Amount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JobcardId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HandOverTo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IssuedDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>

        </tr>
    }

</table>
<div id="aligned">
    @Html.ActionLink("Back","GetVehicleWiseStocksReport",new {id=1})
</div>