@model IEnumerable<Fleet_WorkShop.Models.InfraStructure>
<link href="~/ScriptsCss/Bindings.css" rel="stylesheet" />
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.InfraName)
        </th>
  
        <th>
            @Html.DisplayNameFor(model => model.CategoryName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Quantity)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.InfraName)
        </td>
    
        <td>
            @Html.DisplayFor(modelItem => item.CategoryName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Quantity)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditInfra", new {  id=item.infra_id}) |
            @Html.ActionLink("Delete", "InfraDelete", new { id = item.infra_id })
        </td>
    </tr>
}

</table>
