@model IEnumerable<Aggregates>

<link href="~/ScriptsCss/Bindings.css" rel="stylesheet"/>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SubCategoryName)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.CategoryName)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.ApproxCost)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.SubCategoryName)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.CategoryName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApproxCost)
            </td>
            <td>
                @Html.ActionLink("Edit", "EditSubCategoryCostDetails", new {id = item.ServiceId})

                @*@Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })*@
            </td>
        </tr>
    }

</table>