@model IEnumerable<Fleet_WorkShop.Models.VehicleModel>
<link href="~/ScriptsCss/Bindings.css" rel="stylesheet"/>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.VehicleNumberString)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.DistrictName)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.ManufacturerName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LocationOfCommission)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ChasisNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EngineNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateOfCommission)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModelYear)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.VehicleNumberString)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.DistrictName)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.ManufacturerName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LocationOfCommission)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ChasisNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EngineNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateOfCommission)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModelYear)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new {id = item.Id}) |
                @Ajax.ActionLink("Delete", "Delete", new {id = item.Id}, new AjaxOptions
                {
                    HttpMethod = "Delete",
                    Confirm = "Are you sure you want to delete this student?",
                    OnComplete = "function() { $(this).parent().parent().remove() }"
                })
            </td>
        </tr>
    }

</table>