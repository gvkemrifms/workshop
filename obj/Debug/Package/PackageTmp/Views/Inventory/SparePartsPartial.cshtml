@model IEnumerable<Fleet_WorkShop.Models.SparePartsModel>


<table class="table table-borderless table-striped table-earning">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ManufacturerName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PartName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cost)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ManufacturerName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PartName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cost)
            </td>
            @*<td>
                @Html.ActionLink("Edit", "EditSpares", new {id = item.Id}) |
                @Html.ActionLink("Delete", "DeleteSpares", new {id = item.Id})
            </td>*@
        </tr>
    }

</table>